@{
    var userName = ViewBag.User as string;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - COO Management</title>
    <!-- Custom fonts for this template-->
    <link href="/Content/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <!-- Page level plugin CSS-->
    <link href="/Content/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
    <!-- Custom styles for this template-->
    <link href="/Content/css/sb-admin.css" rel="stylesheet">

    <!--Loading Custome Style -->
    <link href="~/css/site.min.css" rel="stylesheet" />
    <!--Icon App-->
    @*<link rel="icon" type="image/png" href="~/Content/images/iqc.png" />
        <style>
            #dataTable {
