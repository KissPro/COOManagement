</head>

<body id="page-top" class="sidebar-toggled">
    <!-- Loading  -->
    <div id="divLoader" style="display:none"></div>
    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">
        <a class="navbar-brand mr-1" href="/">IQC Inspection</a>
        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button>
        <!-- Navbar Search -->
        <div class="d-none d-md-inline-block form-inline ml-auto mr-0 my-2 my-md-0 text-white-50">
            @userName
        </div>
        <!-- Navbar -->
        <a class="nav-link dropdown-toggle" href="/Login/SignOut" id="userDropdown" role="button">
            <i class="fas fa-sign-out-alt fa-fw"></i>
        </a>
        @*<ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown no-arrow">
                <a class="nav-link dropdown-toggle" href="/Login/SignOut" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-sign-out-alt fa-fw"></i>
                </a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    <a class="dropdown-item text-danger" href="/Login/SignOut" data-toggle="modal" data-target="#logoutModal">Logout</a>
                </div>
            </li>
        </ul>*@
    </nav>
    <div id="wrapper">
        <!-- Sidebar -->
        <ul class="sidebar navbar-nav toggled">
            <li class="nav-item active">
                <a class="nav-link" href="/" onclick="document.getElementById('divLoader').style.display = 'unset'">
                    <i class="fas fa-fw fa-bolt"></i>
                    <span>DN Incoming</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/inspected" onclick="document.getElementById('divLoader').style.display = 'unset'">
                    <i class="fas fa-fw fa-check"></i>
                    <span>DN Created</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link clickMaterial" href="/boom-ecus/all" onclick="document.getElementById('divLoader').style.display = 'unset'">
                    <i class="fas fa-fw fa-coins"></i>
                    <span>Boom - Ecus</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/config/country-show" onclick="document.getElementById('divLoader').style.display = 'unset'">
                    <i class="fas fa-fw fa-exclamation-circle"></i>
                    <span>Country Ship</span>
                </a>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-cogs"></i>
                    <span>Setting</span>
                </a>
                <div class="dropdown-menu" aria-labelledby="pagesDropdown" x-placement="bottom-start" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(5px, 56px, 0px);">
                    <a class="dropdown-item" href="/config/country-show" onclick="document.getElementById('divLoader').style.display = 'unset'">Country Ship</a>
                </div>
            </li>
            @*<li class="nav-item">
                    <a class="nav-link" href="/approval/1">
                        <i class="fas fa-fw fa-user-tie"></i>
                        <span>Function Head</span>
                        @if (TempData["ApprovalTotalL1"] != null && (int)TempData["ApprovalTotalL1"] > 0)
                        {
                            <span class="badge badge-danger" style="position:unset;font-size: 0.7rem;width: 20px;margin-left: 1.2rem;margin-top: 0.2rem">@((int)TempData["ApprovalTotalL1"])</span>
                        }
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/approval/2">
                        <i class="fas fa-fw fa-user-shield"></i>
                        <span>GA Manager</span>
                        @if (TempData["ApprovalTotalL2"] != null && (int)TempData["ApprovalTotalL2"] > 0)
                        {
                            <span class="badge badge-danger" style="position:unset;font-size: 0.7rem;width: 20px;margin-left: 1.2rem;margin-top: 0.2rem">@((int)TempData["ApprovalTotalL2"])</span>
                        }
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/security">
                        <i class="fas fa-fw fa-shield-alt"></i>
                        <span>Sec Maingate</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/receptionist">
                        <i class="fas fa-fw fa-user-check"></i>
                        <span>DIO</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/report">
                        <i class="fas fa-fw fa-mail-bulk"></i>
                        <span>Report</span>
                    </a>
                </li>*@
        </ul>
        <div id="content-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright © <b>Hanoi IT</b> 2020</span>
                    </div>
                </div>
            </footer>
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <a class="btn btn-primary" href="/Login/SignOut">Logout</a>
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!-- Bootstrap core JavaScript-->
    <script src="/Content/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/js/jquery-1.7.min.js"></script>
    <script src="~/Content/js/jquery-ui.min.js"></script>
    <script src="/Content/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="/Content/vendor/jquery-easing/jquery.easing.min.js"></script>
    <!-- Page level plugin JavaScript-->
    <script src="/Content/vendor/datatables/jquery.dataTables.js"></script>
    <script src="/Content/vendor/datatables/dataTables.bootstrap4.js"></script>

    <!-- Download Button -->
    <script src="~/Content/js/DataTables/dataTables.buttons.min.js"></script>
    <script src="~/Content/js/jszip.min.js"></script>
    <script src="~/Content/js/buttons.html5.min.js"></script>

    <!--Show pnotity-->
    @if (TempData["notify-content"] != null)
    {
        <script>
                $(document).ready(function () {
                new PNotify({
                    title: '@TempData["notify-title"]',
                    text: '@TempData["notify-content"]',
                    type: '@TempData["notify-type"]',
                    delay: '3000',
                });
            });
        </script>
    }
    <!--Common Script-->
    <script>
        function Noti(title, message, type) {
            new PNotify({
                title: title,
                text: message,
                type: type,
                delay: '3000',
            });
        }
    </script>

    <!--Add custom js here-->
    <script src="~/js/site.min.js"></script>

    @RenderSection("jsFooter", required: false)
</body>
</html>
